{
  "version": 2,
  "builds": [
    {
      "src": "src/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "status": 404
    },
    {
      "src": "/",
      "status": 404
    },
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ]
}
